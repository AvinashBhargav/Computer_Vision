   "source": [
    "import paddleocr\n",
    "ocr = paddleocr.PaddleOCR(use_angle_classifier=True, lang='en')\n",
    "\n",
    "# Specify the image path\n",
    "img_path = './april.png'\n",
    "\n",
    "# Perform OCR\n",
    "result = ocr.ocr(img_path, det=True)\n",
    "\n",
    "# Print the recognized text\n",
    "for line in result:\n",
    "    for box in line:\n",
    "        print(box[-1])"
